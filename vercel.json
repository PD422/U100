{
  "version": 2,
  "name": "u100-platform",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_ENV": "production",
    "REACT_APP_VERSION": "1.0.0",
    "GENERATE_SOURCEMAP": "false"
  },
  "build": {
    "env": {
      "REACT_APP_ENV": "production",
      "REACT_APP_VERSION": "1.0.0",
      "GENERATE_SOURCEMAP": "false"
    }
  }
}
